  "bugs": {
    "url": "https://github.com/shinho601/nomadcoffee-backend/issues"
  },
  "homepage": "https://github.com/shinho601/nomadcoffee-backend#readme",
  "engines": {
    "node": "16.13.1"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@prisma/client": "^2.16.1",
    "apollo-server": "^2.19.1",
    "apollo-server-express": "^2.19.2",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-capacitor": "^6.2.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "graphql-upload": "^11.0.0",
